{
    "name": "Free Haaretz",
    "description": "Bypass the paywall of Haaretz group",
    "version": "0.2",
    "manifest_version": 3,
    "action": {
        "default_icon": "icon128.png"
    },
    "host_permissions": [
        "https://www.haaretz.co.il/",
        "https://www.themarker.com/"
    ],
    "declarative_net_request": {
        "rule_resources": [
            {
                "id": "ruleset",
                "enabled": true,
                "path": "rules.json"
            }
        ]
    },
    "permissions": [
        "declarativeNetRequest"
    ]
}